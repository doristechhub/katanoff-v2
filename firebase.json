{
  "hosting": [
    {
      "target": "admin",
      "public": "newAdminPanel/dist",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ],
  "functions": {
    "source": "cloudFunction/functions"
  }
}